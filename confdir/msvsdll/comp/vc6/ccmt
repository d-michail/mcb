CC = cl -c -nologo -MT -O -Ob2 -EHsc -TP
PLAIN_C = cl -c -nologo -MT -O -Ob2 -EHsc -TC
LD = cl -nologo -MT
LINKVER = _MT

AR  = lib -nologo 
AR0 = lib -nologo /out:

